import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i,j;
        int n = sc.nextInt();
        int a[] = new int[n];
        for(i=0;i<n;i++){
            a[i]=sc.nextInt();
        }
        for(j=n-1;j>=0;j--){
            System.out.print(a[j] +" ");
            }
        
    }
}
